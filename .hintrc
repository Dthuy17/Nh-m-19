{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-roles": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "widows",
          "text-align: -webkit-match-parent",
          "height: stretch",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "text-decoration-skip-ink",
          "overflow-anchor",
          "scrollbar-width",
          "scrollbar-color",
          "@page",
          "orphans",
          "@page",
          "@page"
        ]
      }
    ]
  }
}